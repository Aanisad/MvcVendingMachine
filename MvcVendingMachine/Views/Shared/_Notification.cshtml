@if (TempData[SD.Success] != null)
{
    <script src="~/lib/jquery/dist/query.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css"></script>
    <script type="text/javascript">
        toastr.success('@TempData[SD.Success]');
    </script>
}
@if (TempData[SD.Error] != null)
{
    <script src="~/lib/jquery/dist/query.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css"></script>
    <script type="text/javascript">
        toastr.error('@TempData[SD.Error]');
    </script>
}